    flags = flags | include_flags
  ## end

  blitSub(
    credits.data,
    dest_x,
    dest_y,
    src_w * units.UNIT,
    src_h * units.UNIT,
    src_x * units.UNIT,
    src_y * units.UNIT,
    credits.width,
    flags
  )
end

function credits.paint_tile(src_x: integer, src_y: integer, dest_x: integer, dest_y: integer)
  credits.paint_sub(src_x, src_y, 1, 1, dest_x, dest_y)
end

return credits


